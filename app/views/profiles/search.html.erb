<!-- app/views/profiles/search.html.erb -->

<h1>Resultados da busca por "<%= @search_term %>"</h1>

<% if @profiles.any? %>
  <ul>
    <% @profiles.each do |profile| %>
      <li><%= profile.name %></li>
      <!-- Adicione mais informações do perfil conforme necessário -->
    <% end %>
  </ul>
<% else %>
  <p>Nenhum perfil encontrado.</p>
<% end %>